#include<stdio.h>
#include<string.h>
int checklen(char str1[],char str2[],char str3[]){
    if(strlen(str3)==(strlen(str1)+strlen(str2)))
        return 1;
    else
        return 0;
}
int checkchar(char str1[],char str2[],char str3[]){
    int count=0,i,j;
    char str[100];
    for(i=0;i<strlen(str1);i++)
        str[count++]=str1[i];
    for(i=0;i<strlen(str2);i++)
        str[count++]=str2[i];
    count=0;
    for(i=0;i<strlen(str3);i++){
        for(j=0;j<strlen(str);j++){
            if(str3[i]==str[j])
                count++;
        }
    }
    if(count==strlen(str3))
        return 1;
    else
        return 0;
}
int checkorder(char str1[],char str2[],char str3[]){
    int i,flag=0,j;
    char strin1[50],strin2[50],count1=0;
    for(i=0;i<strlen(str3);i++){
        for(j=0;j<strlen(str1);j++){
            if(str3[i]==str1[j])
                strin1[count1++]=str3[i];
        }
    }
    
    if(!strcmp(strin1,str1))
        flag+=1;
    count1=0;
    for(i=0;i<strlen(str3);i++){
        for(j=0;j<strlen(str2);j++){
            if(str3[i]==str2[j])
                strin2[count1++]=str3[i];
        }
    }
 
    if(!strcmp(strin2,str2))
        flag+=1;
    
    if(flag==2)
        return 1;
    else 
        return 0;
    
}

int main(){
    char str1[50],str2[50],str3[50],count=0;
    printf("Enter first string:\n");
    scanf("%s",str1);
     printf("Enter second string:\n");
    scanf("%s",str2);
     printf("Enter string to be checked:\n");
    scanf("%s",str3);
    if(checklen(str1,str2,str3))
        count+=1;
    if(checkchar(str1,str2,str3))
        count+=1;
    if(checkorder(str1,str2,str3))
        count+=1;
    if(count==3)
        printf("\nYes\n");
    else
        printf("\nNo\n");
}
