#include<stdio.h>
#include<string.h>
int main(){
    int i=0,j=0,k=0,f=0;
    char str1[50],str2[50],str3[50],count=0;
    printf("Enter first string:\n");
    scanf("%s",str1);
     printf("Enter second string:\n");
    scanf("%s",str2);
     printf("Enter string to be checked:\n");
    scanf("%s",str3);
    if(strlen(str1)+strlen(str2)!=strlen(str3))
        printf("\nNo\n");
    else{
        while(k<strlen(str3)){
            if(i<strlen(str1) && str1[i]==str3[k])
                i++;
            else if(j<strlen(str2)&& str2[j]==str3[k])
                j++;
            else{
                f=1;
                break;
            }
            k++;
        }
        if(i<strlen(str1) || j<strlen(str2) ||f==1)
            printf("\nNo\n");
        else
            printf("\nYes\n");
    }}
