#include<stdio.h>
#include<stdlib.h>
int G[10][10],n,visited[10],count=0;
void DFS(int i){
    printf(" %d ",i);
    count++;
    visited[i]=1;
    for(int j=1;j<=n;j++){
        if(visited[j]!=1 && G[i][j]==1 && G[j][i]!=1)
            DFS(j);
    }
}
void main(){
    int i,j;
    printf("\nEnter number of vertices: \n");
    scanf("%d",&n);
    printf("\nEnter adjacency matrix\n");
    for(i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            scanf("%d",&G[i][j]);
        }
    }
    for(i=1;i<=n;i++){
        visited[i]=0;
    }
    for(i=1;i<=n;i++){
        if(count==n)
            break;
        if(visited[i]!=1)
         DFS(i);
    }
     
    
}
