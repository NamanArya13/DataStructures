#include<stdio.h>
#include<stdlib.h>
int *reversearray(int arr[],int n){
    int i,j,temp;
    for(i=0,j=n-1;i<=j;i++,j--){
        temp=arr[i];
        arr[i]=arr[j];
        arr[j]=temp;
    }
    
    return arr;
}
int main(){
    int array[100],n;
    int *p;
    printf("\nEnter number of elements: \n");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        printf("\nEnter element: ");
        scanf("%d",&array[i]);
        
    }
    p=reversearray(array,n);
    for(int i=0;i<n;i++){
        printf(" %d ",*(p+i));
    
        
}}
